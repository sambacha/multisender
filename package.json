{
  "name": "multi-sender-app",
  "version": "0.2.0",
  "license": "GPL3",
  "homepage": "https://rstormsf.github.io/multisender",
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "csvtojson": "^2.0.10",
    "gh-pages": "^3.1.0",
    "json-validator": "0.0.34",
    "mobx": "^3.6.1",
    "mobx-react": "^7.1.0",
    "node-sass-chokidar": "1.5.0",
    "nodemon": "^2.0.7",
    "react": "^16.9.0",
    "react-app-rewire-mobx": "^1.0.9",
    "react-app-rewire-preact": "^1.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^16.9.0",
    "react-json-view": "^1.21.3",
    "react-radio-group": "^3.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.2.1",
    "react-spinners": "^0.10.6",
    "react-validation": "^3.0.7",
    "splash-screen": "^4.0.1",
    "store2": "^2.12.0",
    "sweetalert": "^2.1.2",
    "web3": "^1.3.4"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.10",
    "chai": "^4.3.3",
    "coveralls": "^3.1.0",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eth-gas-reporter": "^0.2.22",
    "ganache-cli": "^6.12.2",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.6",
    "solc": "^0.4.24",
    "solidity-coverage": "^0.7.16",
    "solium": "^1.2.5",
    "truffle": "^5.2.3",
    "truffle-flattener": "^1.5.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/stylesheets/application.scss -o src/assets/stylesheets --output-style=compressed",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -o rstorm",
    "coverage": "scripts/coverage.sh && cat coverage/lcov.info | coveralls",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sol": "solium -d .",
    "lint:sol:fix": "solium -d . --fix",
    "lint": "yarn run lint:js && yarn run lint:sol",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:sol:fix"
  }
}
